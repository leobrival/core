{
	"name": "@topographic-studio/ui",
	"version": "0.0.1",
	"description": "Reusable UI components for Topographic Studio projects",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"ui",
		"components",
		"react"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/leobrival/core.git",
		"directory": "packages/ui"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"devDependencies": {
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2"
	}
}
